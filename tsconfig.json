{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,        
        "noImplicitAny": true,
        "removeComments": true,
        "module": "commonjs",
        "preserveConstEnums": true,
        "target": "es5",
        "jsx": "react"
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}